%article#founders.section.section--center-content.founders
  .section__container
    .container
      .row
        .founders__left.col-xs-12.col-sm-6
          .founders__content
            .founders__picture
              %img{:alt => 'Alexandru Calinoiu', :height => '85', :src => image_path('common/profile-calin.png'), :width => '85'}
            .founders__contact
              %h3.founders__name Alex
              %span.founders__title #{t('layout.founders.title')}
              %br
              = link_to '+40 745 857 479', '#', class: 'founders__phone', data: { title: 'Call Calin', omnipaste_call: '+40 745 857 479' }
        .founders__right.col-xs-12.col-sm-6
          .founders__content
            .founders__picture
              %img{:alt => 'Adrian Nistor', :height => '85', :src => image_path('common/profile-adi.png'), :width => '85'}
            .founders__contact
              %h3.founders__name Adi
              %span.founders__title #{t('layout.founders.title')}
              %br
              = link_to '+40 747 074 372', '#', class: 'founders__phone', data: { title: 'Call Adi', omnipaste_call: '+40 747 074 372' }
